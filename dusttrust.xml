<?xml version="1.0" encoding="UTF-8"?>

<!-- Defines the ISO project -->
<iso_project image_name="dusttrust.bin" cue_sheet="dusttrust.cue" no_xa="0">
	<!-- Data track -->
	<track type="data">
		<!-- Identifiers -->
		<identifiers
			system        = "PLAYSTATION"
			application   = "PLAYSTATION"
			volume        = "DUSTTRUST"
			volume_set    = "PRE-LEAK"
			publisher     = "LUCKY"
			copyright     = "NICKOLAS"
		/>
		<license file="licensea.dat"/>	
	
		<!-- Data directory tree -->
		<directory_tree>
			<!-- System -->
			<file name = "system.cnf" type = "data" source = "iso/system.cnf"/>
			<file name = "SCUS_000.00" type = "data" source = "dusttrust.ps-exe"/>

			<!-- Sprites -->
			<dir name = "sprites">
				<file name = "dtsans.tim" type = "data" source = "iso/sprites/dtsans.tim"/>
			</dir>	

			<!-- Music -->
			<dir name = "music">
				<file name = "phase1.mus" type = "data" source = "iso/music/homicidal-lunacy/homicidal-lunacy.mus"/>
			</dir>			

			<!-- Dummy sectors -->
			<dummy sectors="1024"/>
		</directory_tree>
	</track>
</iso_project>
